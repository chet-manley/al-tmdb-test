{
    "useTabChar": true,
    "tabSize": 4,
    "spaceUnits": 4,
    "smartIndent": true,
    "insertHintOnTab": false,
    "linting.enabled": true,
    "jslint.options": {
        "predef": [
            "angular"
        ],
        "browser": true,
        "debug": true,
        "devel": true,
        "node": false,
        "todo": true,
        "rhino": false,
        "windows": false,
        "anon": false,
        "ass": false,
        "bitwise": false,
        "eqeq": false,
        "evil": false,
        "es5": true,
        "forin": false,
        "indent": 4,
        "maxerr": 20,
        "newcap": false,
        "nomen": false,
        "passfail": false,
        "plusplus": false,
        "regexp": false,
        "sloppy": false,
        "stupid": false,
        "undef": false,
        "unparam": false,
        "vars": false,
        "white": false
    }
}